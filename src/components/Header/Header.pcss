#header {
  color: var(--color-coral);
  font-weight: bold;
  padding: 25px 50px;
  box-shadow: none;
  display: flex;
  flex-direction: row;
  align-items: flex-end;
}

.title > a {
  padding-right: 50px;
  font-family: var(--font-poppins);
}

.title:first-child {
  margin-right: 50px;
}

.nav-link {
  font-weight: bold;
}

.active-link {
  color: var(--color-dark);
}

.section {
  height: 30px;
  width: 30px;
  fill: var(--color-coral);
  display: inline-block;
}
